<h3 class="ui header">{{t "forms.i-i-s-logistika-snova-загруз-контейн-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-logistika-snova-загруз-контейн-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерДокЗагк
      class=(if (v-get validationModel "номерДокЗагк" "isInvalid") "error")
      label=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.номерДокЗагк-caption")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-номерДокЗагк=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерДокЗагк" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-logistika-snova-загруз-контейн-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      displayAttributeName="фИОСотруд"
      autocomplete=true
      relationName="сотрудники"
      projection="СотрудникиL"
      title=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-logistika-snova-загруз-контейн-e.контейнеры-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.контейнеры
      displayAttributeName="номерКонтей"
      autocomplete=true
      relationName="контейнеры"
      projection="КонтейнерыL"
      title=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.контейнеры-caption")
      readonly=readonly
      componentName="контейнерыLookup"
      class=(if (v-get validationModel "контейнеры" "isInvalid") "error")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-контейнеры=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "контейнеры" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.контейнеры.объем
      class=(if (v-get validationModel.контейнеры "объем" "isInvalid") "error")
      label=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.контейнеры.объем-caption")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-контейнеры-объем=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.контейнеры "объем" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.объемСвобод
      class=(if (v-get validationModel "объемСвобод" "isInvalid") "error")
      label=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.объемСвобод-caption")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-объемСвобод=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "объемСвобод" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.контейнеры.максВес
      class=(if (v-get validationModel.контейнеры "максВес" "isInvalid") "error")
      label=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.контейнеры.максВес-caption")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-контейнеры-максВес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.контейнеры "максВес" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.остатокПоВесу
      class=(if (v-get validationModel "остатокПоВесу" "isInvalid") "error")
      label=(t "forms.i-i-s-logistika-snova-загруз-контейн-e.остатокПоВесу-caption")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-остатокПоВесу=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "остатокПоВесу" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-logistika-snova-загруз-контейн-e.тЧИнфОГрузе-caption"}}</label>
    {{flexberry-groupedit
      componentName="тЧИнфОГрузеGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.тЧИнфОГрузе
      content=model.тЧИнфОГрузе
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "тЧИнфОГрузе" "isInvalid") "error")
      data-test-i-i-s-logistika-snova-загруз-контейн-e-тЧИнфОГрузе=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧИнфОГрузе" "message")}}
  </div>
</form>
